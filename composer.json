{
    "name": "hassnainabass/users-permissions",
    "description": "Users Permissions Management plugin for CakePHP with GUI",
    "type": "cakephp-plugin",
    "keywords": ["cakephp","users","user permissions"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Hassnain Abass",
            "email": "hussnain.abass@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "3.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "UsersPermissions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UsersPermissions\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
